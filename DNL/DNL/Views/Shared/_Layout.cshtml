<!DOCTYPE html>
<html lang="uk">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="shortcut icon" href="~/logo.ico" type="image/x-icon">
    <title>Долинський науковий ліцей №1</title>

    <environment include="Staging, Production">
        <link type="text/css" rel="stylesheet" href="~/css/bundles/layout.min.css" />
        <link type="text/css" rel="stylesheet" href="~/lib/font-awesome.min.css" />
    </environment>

    <environment include="Development">
        <link type="text/css" rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.min.css" />
        <link type="text/css" rel="stylesheet" href="~/css/layout.css" />
        <link type="text/css" rel="stylesheet" href="~/lib/font-awesome.min.css" />
    </environment>

    <!-- add css files from partial views -->
    @RenderSection("CssFiles", required: false)

</head>
<body>

    <div class="loaderArea">
        <div class="loader">
            <div class="container-fluid ">
                <div id="block-spinner">
                    <div id="loading">

                        <div class="sk-fading-circle">
                            <div class="sk-circle1 sk-circle"></div>
                            <div class="sk-circle2 sk-circle"></div>
                            <div class="sk-circle3 sk-circle"></div>
                            <div class="sk-circle4 sk-circle"></div>
                            <div class="sk-circle5 sk-circle"></div>
                            <div class="sk-circle6 sk-circle"></div>
                            <div class="sk-circle7 sk-circle"></div>
                            <div class="sk-circle8 sk-circle"></div>
                            <div class="sk-circle9 sk-circle"></div>
                            <div class="sk-circle10 sk-circle"></div>
                            <div class="sk-circle11 sk-circle"></div>
                            <div class="sk-circle12 sk-circle"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="non-loader-section">
        <header>
            <nav class="navbar navbar-expand-lg navbar-light fixed-top">
                <div class="container">
                    <a class="navbar-brand" asp-controller="Home" asp-action="Index"><img src="~/images/logo.png" height=35 alt="logo"></a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbar1" aria-controls="navbar1" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbar1">
                        <ul class="navbar-nav ml-auto">
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Інформація</a>
                                <div class="dropdown-menu">
                                    <a class="dropdown-item" asp-controller="Home" asp-action="Statut">Статут наукового ліцею</a>
                                    <a class="dropdown-item" asp-controller="Home" asp-action="License">Ліцензія</a>
                                    <a class="dropdown-item" asp-controller="Home" asp-action="Vacancies">Вакансії</a>
                                    <a class="dropdown-item" asp-controller="Home" asp-action="EntryRules">Правила вступу</a>
                                </div>
                            </li>
                            <li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Управління</a>
                                <div class="dropdown-menu">
                                    <a class="dropdown-item" asp-controller="Teacher" asp-action="Administration">Адміністрація закладу</a>
                                    <a class="dropdown-item" asp-controller="Home" asp-action="WorkPlan">Річний план роботи</a>
                                    <a class="dropdown-item" asp-controller="Home" asp-action="PedagogicalMeetings">Педагогічні ради</a>
                                    <a class="dropdown-item" asp-controller="Home" asp-action="DirectorMeeting">Наради при директорі</a>
                                    <a class="dropdown-item" asp-controller="Home" asp-action="Orders">Накази</a>
                                    <a class="dropdown-item" asp-controller="Home" asp-action="FinancialStatements">Фінансова звітність</a>
                                </div>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-controller="News" asp-action="AllNews">Новини</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" asp-controller="Home" asp-action="Contact">Контакти</a>
                            </li>
                        </ul>
                        @if (User?.Identity?.IsAuthenticated ?? false)
                        {
                            <a class="btn" asp-controller="Profile" asp-action="Index" id="login-button" role="button">Кабінет</a>
                        }
                        else
                        {
                            <a class="btn" asp-controller="Account" asp-action="Login" id="login-button" role="button">Увійти</a>
                        }

                    </div>
                </div>
            </nav>
        </header>

        @RenderBody()

        <!-- Footer -->
        <footer>
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-md-4">
                        <ul class="footer-nav_menu">
                            <li class="title">Детальніше про ліцей</li>
                            <li><a asp-controller="Home" asp-action="About">Долинський науковий ліцей №1</a></li>
                            <li><a asp-controller="Home" asp-action="Contact">Контакти</a></li>
                        </ul>
                    </div>
                    <div class="col-md-3">
                        <ul class="footer-nav_menu">
                            <li class="title">Посилання</li>
                            <li><a asp-controller="News" asp-action="AllNews">Новини</a></li>
                            <li><a asp-controller="PhotoGallery" asp-action="AllAlbums">Фотогалерея</a></li>
                        </ul>
                    </div>
                    <div class="col-md-3">
                        <ul class="footer-nav_menu">
                            <li class="title">Наш ліцей</li>
                            <li><a asp-controller="Timetable" asp-action="Index">Розклад</a></li>
                            <li><a asp-controller="Home" asp-action="MethodicalWork">Методична робота</a></li>
                            <li><a asp-controller="Home" asp-action="EducationalActivities">Виховна робота</a></li>
                            <li><a asp-controller="Home" asp-action="Achievements">Наші досягнення</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </footer>



        <section id="follow-us-on">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="follow-text">Слідкуйте за нами в соціальних мережах</div>
                    <a target="_blank" href="https://www.facebook.com/groups/1724650547661263/"><img alt="" class="icon" src="data:image/png;base64," /></a>
                    <a target="_blank" href="https://www.instagram.com/_.dnl1._"><img alt="" class="icon" src="data:image/png;base64," /></a>
                    <a target="_blank" href="https://www.youtube.com/channel/UCigQW6Rz2vH62G3yTZ-OEkw"><img alt="" class="icon" src="data:image/png;base64," /></a>
                    <a target="_blank" href="https://uk.wikipedia.org/wiki/%D0%94%D0%BE%D0%BB%D0%B8%D0%BD%D1%81%D1%8C%D0%BA%D0%B8%D0%B9_%D0%BD%D0%B0%D1%83%D0%BA%D0%BE%D0%B2%D0%B8%D0%B9_%D0%BB%D1%96%D1%86%D0%B5%D0%B9_%E2%84%96_1"><img alt="" class="icon" src="data:image/png;base64," /></a>

                </div>
            </div>
        </section>
    </div>
    

    <environment include="Staging, Production">
        <script src="~/js/bundles/layout.min.js"></script>
    </environment>

    <environment include="Development">
        <script src="~/lib/jquery.js"></script>
        <script src="~/lib/jquery.validate.js"></script>
        <script src="~/lib/jquery.validate.unobtrusive.js"></script>
        <script src="~/lib/tether.js"></script>
        <script src="~/lib/bootstrap/js/bootstrap.min.js"></script>
        <script src="~/js/layout.js"></script>
    </environment>

    <!-- add js files from partial views -->
    @RenderSection("JsFiles", required: false)
</body>
</html>
