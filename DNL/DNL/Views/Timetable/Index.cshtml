@section CssFiles {
    <environment include="Staging, Production">
        <link type="text/css" rel="stylesheet" href="~/css/bundles/timetable.min.css" />
    </environment>

    <environment include="Development">
        <link type="text/css" rel="stylesheet" href="~/css/timetable.index.css" />
        <link href="~/lib/bootstrap-select/css/bootstrap-select.css" rel="stylesheet" />
    </environment>
}

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="heading adaptive-heading">
                <h2>Розклад</h2>
            </div>
        </div>
    </div>
</div>

<div class="container">
    <img src="~/images/timetable/1.jpeg" class="w-100" />
    <img src="~/images/timetable/2.jpeg" class="w-100" />
</div>

@section JsFiles {

    <environment include="Staging, Production">
        <script src="~/js/bundles/timetable.min.js" type="text/javascript"></script>
    </environment>

    <environment include="Development">
        <script src="~/js/timetable.js" type="text/javascript"></script>
        <script src="~/lib/bootstrap-select/js/bootstrap-select.js"></script>
    </environment>
}
